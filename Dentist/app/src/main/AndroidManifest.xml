<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="doctor.task.dentist">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.features.patient.clinic.ReserveAppointmentActivity"></activity>
        <activity android:name=".view.features.patient.clinic.SpecialtyActivity" />
        <activity android:name=".view.features.patient.clinic.ClinicDetailsActivity" />
        <activity android:name=".view.features.patient.notification.NotificationActivity" />
        <activity android:name=".view.features.doctor.patientFile.PatientFileActivity" />
        <activity android:name=".view.features.patient.MainActivity" />
        <activity android:name=".view.features.doctor.ReservationListActivity" />
        <activity android:name=".view.registration.RegistrationActivity" />
        <activity android:name=".view.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>